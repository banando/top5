<p id = "notice"><%= notice %></p>
<p>
  <% if current_user %>
    Welcome, <%= current_user.user_name %>
    &nbsp;&nbsp;|&nbsp;&nbsp;
  <td><%= link_to 'Log Out', session_path('end'), method: :delete, data: {confirm: "are you sure?"} %></td>
  <% else %>
  <td><%= link_to 'New User', new_user_path %></td>
      &nbsp;&nbsp;|&nbsp;&nbsp;
  <td><%= link_to 'Login', new_session_path   %></td>
<% end %>
</p>

<h1>TOP 5</h1>
<div class="jumbotron">
  <div class="container">
    <h1>TOP 5</h1>
  </div>
</div>

<p>
  <%= link_to "add your own Top 5", new_list_path %>
</p>
<table>
  <thread>
    <tr>
      <th>Topic</th>
    </tr>
  </thread>
  <tbody>
    <% @lists.each do |list| %>
        <tr><td><%= list.topic %></td></tr>
        <tr><td>1. <%= list.first %> </td></tr>
        <tr><td>2. <%= list.second %> </td></tr>
        <tr><td>3. <%= list.third %> </td></tr>
        <tr><td>4. <%= list.fourth %> </td></tr>
        <tr><td>5. <%= list.fifth %> </td></tr>
        <tr><td><%= link_to 'Show', list %></td></tr>
        <tr><td><%= link_to 'Delete', list, method: :delete, data: {confirm: 'Are you sure'} %></td></tr>

    <% end %>

  </tbody>

</table>
